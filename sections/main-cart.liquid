{% capture cartNote %}
    {% if settings.cartnote %}
        <cart-note class="db cart__note">
            <label class="fwm mb15 ttu" for="Cart-note">{{ 'cart.general.note' | t }}</label>
            <textarea class="text-area mb20 w_100" name="note" form="cart" id="Cart-note" placeholder="{{ 'cart.general.note' | t }}">{{ cart.note }}</textarea>
        </cart-note>
    {% endif %}
{% endcapture %}
<div class="sec_cov" style="--sst:{{ secSt.sst }}px;--ssb:{{ secSt.ssb }}px;--sstm:{{ secSt.sstm }}px;--ssbm:{{ secSt.ssbm }}px">
    <div class="db page-width {% if cart == empty %}is-empty{% endif %}" style="max-width:{{ section.settings.pagewidth }}px">
            <h1 class="page-title tc">{{ 'cart.general.title' | t }}</h1>
            <div class="cart__warnings tc{% if cart != empty %} dn{% endif %}">
                {% render 'icon-cart' %}
                <p>{{ 'cart.general.empty' | t }}. <a class="btnLink" href="{{ routes.all_products_collection_url }}">{{ 'cart.button.continue_shopping' | t }}</a></p>              
                {%- if shop.customer_accounts_enabled and customer == nil -%}
                    <h2 class="mb5">{{ 'cart.login.title' | t }}</h2>
                    <p>{{ 'cart.login.paragraph_html' | t: link: routes.account_login_url  | replace: '/customer_authentication/redirect', '/account/login' }}</p>
                {%- endif -%}
                <p>&nbsp;</p><p>&nbsp;</p>
            </div>
            {% if section.settings.cartCount %}       
              <div class="cartCountdown tc mb25 fl f-jcc f-aic" data-countdown="{{ secSt.countTime | times: 60 }}">
  			<svg class="at-icon iconl mr5 hide-sm" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" data-name="Layer 1"><path d="m16 29a13 13 0 1 1 13-13 13.015 13.015 0 0 1 -13 13zm0-24a11 11 0 1 0 11 11 11.013 11.013 0 0 0 -11-11z"/><path d="m16 22a1 1 0 0 1 -1-1v-7a1 1 0 0 1 2 0v7a1 1 0 0 1 -1 1z"/><circle cx="16" cy="11" r="1"/></svg>
    			<p> {{ secSt.countMsg | replace:"[time]","<b id='cartTime' class='ml5 mr5'></b>" }}</p>
            </div>
            {% endif %}
        
            <div class="cart{% if secSt.layout == "2" %} grid style2{% else %} style1{% endif %}">

                {% if secSt.layout == "1" %}
                    {{ cartTable }}    
                    <ul class="fl cart_footer{% if cart == empty %} is-empty{% endif %}">
                        <li class="cart_notes">
                            {{ giftWrap }}
                            {{ cartNote }}                           
                        </li>
                        <li class="cartSummery">{{ cartSummery }}</li>
                    </ul>
                {% else %}
                    <div class="gitem wd66-lg">
                        {{ cartTable }}
                        {{ giftWrap }}
                    </div>
                    <div class="gitem wd33-lg{% if cart == empty %} is-empty{% endif %}" id="main-cart-footer" data-id="{{ section.id }}">
                        <ul class="cart_footer">
                            <li class="cart_notes">{{ cartNote }}</li>                           
                            <li class="cartSummery">{{ cartSummery }}</li>
                        </ul>
                    </div>
                {% endif %}
            </div>
            {% if section.settings.shipCalc %}{% render 'shipping-calculator' %}{% endif %}
        </div>
    </div>
</div>
<script>
  window.addEventListener('DOMContentLoaded',function(){
  	{%- if settings.terms_checkbox -%}$('#cartTearm').change(function(e){if(this.checked == true){ $('.cartCheckout').removeAttr("disabled"); } else { $('.cartCheckout').attr("disabled", "disabled"); }});{%- endif -%}
});
</script>

