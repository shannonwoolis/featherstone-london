					var $shopifypaymentbutton = $('.shopify-payment-button').find(".shopify-payment-button__button");
					if($shopifypaymentbutton.length){
						$buyinoneclick.hide();
						setTimeout(function(){
							$(".shopify-payment-button__button").attr('disabled', true);
